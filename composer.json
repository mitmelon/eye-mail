    },
    {
      "type": "vcs",
      "url": "https://github.com/mitmelon/Vars-Updated"
    },
    {
      "type": "vcs",
      "url": "https://github.com/mitmelon/yasumi"
    },
    {
      "type": "vcs",
      "url": "https://github.com/mitmelon/Filebase"
    },
    {
      "type": "vcs",
      "url": "https://github.com/mitmelon/2fa-library"
    }
