    "jpkleemans/htaccess-firewall": "dev-master",
    "kriswallsmith/buzz": "^1.1",
    "laminas/laminas-diactoros": "^2.8",
    "laminas/laminas-httphandlerrunner": "^2.1",
    "lbuchs/webauthn": "^1.1",
    "league/csv": "^9.8",
    "league/mime-type-detection": "^1.11",
    "league/route": "^5.1",
    "longman/telegram-bot": "^0.81.0",
    "m1/vars": "dev-master",
    "maennchen/zipstream-php": "^3.0",
    "matomo/device-detector": "^4.2.3",
    "mitmelon/compress": "^2.0",
    "monolog/monolog": "^1.24",
    "mpdf/mpdf": "^8.1",
