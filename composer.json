    "php-curl-class/php-curl-class": "^9.13",
    "php-http/curl-client": "^2.2",
    "php-http/discovery": "*",
    "php-http/guzzle7-adapter": "*",
    "php-http/message": "^1.13",
    "rap2hpoutre/similar-text-finder": "^0.2.0",
    "shieldon/simple-cache": "^1.3",
    "symfony/finder": "^5.1@beta",
    "symfony/mailer": "^6.1",
    "tmarois/filebase": "dev-master",
    "unitpay/shamir": "^1.0"
